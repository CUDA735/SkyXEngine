
#include <../struct.h>

half4 Param;
half4 NearFarIsUnlit;

ps_out_ds_mrt main(vs_out_gcommon IN)
{
	ps_out_ds_mrt OUT; 
	OUT.Color = Param;
	OUT.Color.a = 1;
	OUT.Normal = half4(0,0,0,NearFarIsUnlit.z);
	OUT.Param = half4(0,0,NearFarIsUnlit.w, 0);
	
	OUT.Depth = GetDepthW(IN.Pos, NearFarIsUnlit.xy);
	return OUT;
}